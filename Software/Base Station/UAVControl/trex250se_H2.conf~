ID=1 					% HELICOPTER IDENTIFICATION
IP_ADDRESS=192.168.1.2			% HELICOPTER IP ADDRESS
IP_PORT=2002				% HELICOPTER IP PORT
PC_PORT=2002				% PC PORT HELICOPTER COMMUNICATES THROUGH
BROADCAST_PORT=12345			% PORT PROCESSED DATA IS BROADCASTED THROUGH
INITIAL_POSE=0,-500,45,0,1,0,0		% INITIAL POSE OF HELICOPTER [Tx,Ty,Tz,q0,q1,q2,q3] TRANSLATION IN MILLIMETERS
RED_BLOB=-150,260,0			% POSITION OF RED BLOB W.R.T. HELICOPTER FRAME ORIGIN IN MILLIMETERS
GREEN_BLOB=-145,-260,0			% POSITION OF GREEN BLOB W.R.T. HELICOPTER FRAME ORIGIN IN MILLIMETERS
BLUE_BLOB=310,0,0			% POSITION OF BLUE BLOB W.R.T. HELICOPTER FRAME ORIGIN IN MILLIMETERS
GYRORAW2DEGREES=14.375,14.375,14.375	% GYRO RAW UNITS/DEGREES PER SECOND i.e. RAW READINGS ARE DIVIDED BY THIS NUMBER
ACCELRAW2G=272,258,225			% ACCELEROMETER OUTPUT FOR EACH AXIS THAT IS EQUIVALENT TO 1G
